[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root  # Изменил на root, так как у вас в плейбуке используется root
Group=root
WorkingDirectory={{ project_path }}
ExecStart={{ virtualenv_path }}/bin/gunicorn --workers 3 --bind unix:{{ project_path }}/gunicorn.sock snpProject.wsgi:application

[Install]
WantedBy=multi-user.target